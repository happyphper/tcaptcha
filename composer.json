{
    "name": "happyphper/tcaptcha",
    "description": "TCaptcha",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Happyphper\\TCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Happyphper\\TCaptcha\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Baron"
        }
